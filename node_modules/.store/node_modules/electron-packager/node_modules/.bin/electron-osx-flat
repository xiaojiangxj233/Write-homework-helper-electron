#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/xueqianduan/rzxzy-electron/Write-homework-helper-electrons/node_modules/.store/@electron+osx-sign@1.0.4/node_modules/@electron"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/xueqianduan/rzxzy-electron/Write-homework-helper-electrons/node_modules/.store/@electron+osx-sign@1.0.4/node_modules/@electron"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@electron+osx-sign@1.0.4/node_modules/@electron/osx-sign/bin/electron-osx-flat.js" "$@"
else
  exec node  "$basedir/../../../../../@electron+osx-sign@1.0.4/node_modules/@electron/osx-sign/bin/electron-osx-flat.js" "$@"
fi
