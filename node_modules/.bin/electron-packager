#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/xueqianduan/rzxzy-electron/Write-homework-helper-electrons/node_modules/.store/electron-packager@17.1.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/xueqianduan/rzxzy-electron/Write-homework-helper-electrons/node_modules/.store/electron-packager@17.1.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/electron-packager@17.1.1/node_modules/electron-packager/bin/electron-packager.js" "$@"
else
  exec node  "$basedir/../.store/electron-packager@17.1.1/node_modules/electron-packager/bin/electron-packager.js" "$@"
fi
